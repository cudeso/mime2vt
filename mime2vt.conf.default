[virustotal]
apikey: <your_virustotal_api_key>
exclude: image/png,image/gif,image/jpeg,text/plain,text/html

[elasticsearch]
server: 127.0.0.1:9200
index: mhn 
doctype: event 
#doctype: VTresult

[database]
dbpath: /var/tmp/mime2vt.db
#skip: True 
skip: False 

[MHN]
dest_ip: 192.168.171.1
dest_port: 25
app: spamtrap
vendor_product: spamtrap
ids_type: spamtrap
